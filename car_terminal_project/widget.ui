}
QPushButton#btn_snake:hover {background-color: #1D4ED8;}/* 悬浮加深，无位移 */
QPushButton#btn_snake:pressed {background-color: #1E40AF;}/* 按下更深，无位移 */

QPushButton#btn_plane {
    background-color: #DC2626;/* 红色按钮，参考图配色 */
